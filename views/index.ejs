<%- include header %>
 <%posts.forEach(function(post,index){
	var date=new Date(post.time);
 %>

    <p><h2><a href="/u/<%=post.name%>/<%=post.title%>"><%=post.title%><a></a></h2></p>
    <p class="info">
        作者:<a href="/u/<%=post.name%>"><%=post.name%></a>
        日期:<%=date.getFullYear()+"-"+date.getMonth()+"-"+date.getDay()+"  "+date.getHours()+":"+date.getMinutes()+":"+date.getSeconds()%>
    </p>
    <p><%- post.post%></p>
<%});%>
<%- include paging %>
<%- include footer %>
